.custom-tabbar {
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 120rpx;
  display: flex;
  background: #fff;
  box-shadow: 0 -2rpx 10rpx rgba(0, 0, 0, 0.06);
}

.custom-tabbar .tab-item {
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.custom-tabbar .tab-item .tab-icon {
  width: 48rpx;
  height: 48rpx;
}

.custom-tabbar .tab-item .tab-text {
  font-size: 20rpx;
  margin-top: 4rpx;
}

.active .tab-text {
  color: #34d6f0;
}